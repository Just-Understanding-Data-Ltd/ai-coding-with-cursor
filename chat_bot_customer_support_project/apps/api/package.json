    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "load-env": "dotenv -e .env --",
    "start": "pnpm run load-env node dist/server.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
